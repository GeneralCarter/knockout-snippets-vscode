	"kodbv": {
		"prefix": "kodbv",
		"body": "data-bind=\"visible: $1\"\n",
		"description": "The visible binding  `data-bind='visible: '`",
		"scope": "text.html"
	},
	"kodbt": {
		"prefix": "kodbt",
		"body": "data-bind=\"text: $1\"",
		"description": "The textInput binding  `data-bind='text: '`",
		"scope": "text.html"
	},
	"kodbh": {
		"prefix": "kodbh",
		"body": "data-bind=\"html: $1\"",
		"description": "The html binding  `data-bind='html: '`",
		"scope": "text.html"
	},
	"kodbc": {
		"prefix": "kodbc",
		"body": "data-bind=\"css: $1\"",
		"description": "The css binding  `data-bind='css: '`",
		"scope": "text.html"
	},
	"kodbs": {
		"prefix": "kodbs",
		"body": "data-bind=\"style: $1\"",
		"description": "The style binding  `data-bind='style: '`",
		"scope": "text.html"
	},
	"kodba": {
		"prefix": "kodba",
		"body": "data-bind=\"attr: $1\"",
		"description": "The attr binding  `data-bind='attr: '`",
		"scope": "text.html"
