	"kodbe": {
		"prefix": "kodbe",
		"body": "data-bind=\"${1:click}: $2\"",
		"description": "data-bind event",
		"scope": "text.html"
	},
	"kodbf": {
		"prefix": "kodbf",
		"body": "data-bind=\"${1:click}: function($2) { $3 }\"",
		"description": "data-bind function call",
		"scope": "text.html"
	},
	"kodbtmp": {
		"prefix": "kodbtmp",
		"body": " data-bind='template: {name: \"$1\", ${2:foreach: $3}}'",
		"description": "data-bind template",
		"scope": "text.html"
	},
	"kodbt": {
		"prefix": "kodbt",
		"body": "data-bind=\"text: $1\"",
		"description": "The textInput binding  `data-bind='text: '`",
		"scope": "text.html"
	},
	"kodbv": {
		"prefix": "kodbv",
		"body": "data-bind=\"value: $1\"\n",
		"description": "data-bind value",
		"scope": "text.html"
	}
}